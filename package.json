{
  "name": "11ty-starter-template",
  "version": "1.0.0",
  "description": "11ty-starter-template - built with Eleventy and TailwindCSS",
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "set ELEVENTY_ENV=development && start /b npx @11ty/eleventy --serve --port=8027 & start /b webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "devl": "ELEVENTY_ENV=development npx @11ty/eleventy --serve --port=8027 & webpack --watch  --watch-poll --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "SET NODE_ENV=production&& webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && npx @11ty/eleventy",
    "prodl": "npx webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && npx @11ty/eleventy",
    "debug": "set DEBUG=* & npx @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sharmasourabh/11ty-starter-template.git"
  },
  "author": "Sourabh Sharma",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "common-tags": "^1.8.0",
    "date-fns": "^2.9.0",
    "elasticlunr": "^0.9.5",
    "html-minifier": "^4.0.0",
    "laravel-mix": "^5.0.1",
    "laravel-mix-purgecss": "^4.2.0",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "tailwindcss": "^1.2.0",
    "vue-template-compiler": "^2.6.11"
  }
}
